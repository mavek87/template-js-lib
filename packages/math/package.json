{
  "name": "@com.matteoveroni/math",
  "version": "2.7.0",
  "description": "Math utility library",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vitest",
    "test": "vitest run",
    "lint": "tsc",
    "clean": "rm -rf ./dist",
    "check": "tsc && pnpm test",
    "minify": "esbuild ./dist/index.mjs --minify --outfile=./dist/index.mjs --allow-overwrite --platform=neutral && esbuild ./dist/index.js --minify --outfile=./dist/index.js --allow-overwrite --platform=neutral",
    "build": "pnpm clean && tsup index.ts --format cjs,esm --dts",
    "package": "pnpm check && pnpm build && pnpm minify"
  },
  "keywords": [],
  "author": "com.matteoveroni",
  "license": "ISC",
  "devDependencies": {
    "esbuild": "^0.19.2",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.3"
  },
  "publishConfig": {
    "registry": "http://rebirth-project.it:4873/"
  }
}
